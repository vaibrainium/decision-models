    "%%time\n",
    "\n",
    "coh, sp, dg, sigma, dr, ndt, bound, stop_time = input_for_basic_DDM_implementation()\n",
    "\n",
    "# Model output\n",
    "nTrials = 1000\n",
    "rt = np.array([],dtype=float32)             # Collect reaction time for each trial\n",
    "decision = np.array([],dtype=float32)                  # Collect decision on each trial\n",
